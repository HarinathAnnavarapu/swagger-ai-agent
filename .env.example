# Example environment variables for Swagger AI Agent

# Server
PORT=3000
NODE_ENV=development

# LLM provider: none | openai | azure
LLM_PROVIDER=none
OPENAI_API_KEY=

# Persistence: inmemory | mongo
PERSISTENCE=inmemory
MONGO_URL=

# Limits and safety
SPEC_UPLOAD_MAX_SIZE=2000000
ALLOWED_BASE_URLS=localhost,example.com

# Logging
LOG_LEVEL=info

# Other flags
ENABLE_EXECUTION=false
