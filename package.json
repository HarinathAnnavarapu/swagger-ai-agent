{
  "name": "swagger-ai-agent",
  "version": "0.1.0",
  "private": true,
  "main": "dist/core/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/core/server.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/core/server.js",
    "test": "jest",
    "test:integration": "jest tests/integration --runInBand --testTimeout=30000",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"**/*.{ts,js,json,md}\""
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "undici": "^6.3.0",
    "zod": "^3.21.4",
    "mongodb": "^5.8.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.2",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.5.1",
    "eslint": "^8.51.0",
    "jest": "^29.6.1",
    "prettier": "^3.5.0",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.0"
  }
}
